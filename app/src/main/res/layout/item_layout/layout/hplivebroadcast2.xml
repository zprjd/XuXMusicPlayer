<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    android:id="@+id/wwwwwwwwwwwwwwwwwwwwwwwwwwwwww"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:fillViewport="true"
    android:layout_height="match_parent"
    app:layout_behavior="com.google.android.material.appbar.AppBarLayout$ScrollingViewBehavior"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/pppppppppppppppppppp"
        android:layout_marginTop="30dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:id="@+id/hear"
            android:layout_width="26dp"
            android:layout_height="26dp"
            android:layout_marginStart="58dp"
            android:background="@drawable/earphone"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints" />
        <TextView
            android:id="@+id/heartext"
            android:layout_width="0dp"
            android:textSize="13dp"
            android:textColor="@color/black"
            android:layout_height="wrap_content"
            android:text="听见"
            app:layout_constraintTop_toBottomOf="@+id/hear"
            app:layout_constraintEnd_toEndOf="@+id/hear"
            app:layout_constraintStart_toStartOf="@+id/hear" />
        <ImageView
            android:id="@+id/seen"
            android:layout_width="26dp"
            android:layout_height="26dp"
            android:layout_marginStart="58dp"
            android:background="@drawable/photography"
            app:layout_constraintStart_toEndOf= "@+id/hear"
            tools:ignore="MissingConstraints,UnknownId" />
        <TextView
            android:id="@+id/seentext"
            android:layout_width="0dp"
            android:textSize="13dp"
            android:textColor="@color/black"
            android:layout_height="wrap_content"
            android:text="听见"
            app:layout_constraintTop_toBottomOf="@+id/seen"
            app:layout_constraintEnd_toEndOf="@+id/seen"
            app:layout_constraintStart_toStartOf="@+id/seen" />
        <ImageView
            android:id="@+id/mkfriends"
            android:layout_width="26dp"
            android:layout_height="26dp"
            android:layout_marginEnd="58dp"
            android:background="@drawable/marriageseeking"
            app:layout_constraintEnd_toStartOf="@+id/champion"
            tools:ignore="MissingConstraints" />
        <TextView
            android:id="@+id/mkfriendstext"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="交友"
            android:textSize="13dp"
            android:textColor="@color/black"
            app:layout_constraintTop_toBottomOf="@+id/mkfriends"
            app:layout_constraintEnd_toEndOf="@+id/mkfriends"
            app:layout_constraintStart_toStartOf="@+id/mkfriends" />
        <ImageView
            android:id="@+id/champion"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginEnd="58dp"
            android:background="@drawable/champion"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="MissingConstraints" />
        <TextView
            android:id="@+id/championtext"
            android:layout_width="0dp"
            android:textSize="13dp"
            android:textColor="@color/black"
            android:layout_height="wrap_content"
            android:text="榜单"
            app:layout_constraintTop_toBottomOf="@+id/champion"
            app:layout_constraintEnd_toEndOf="@+id/champion"
            app:layout_constraintStart_toStartOf="@+id/champion" />
        <View
            android:id="@+id/photo"
            app:layout_constraintTop_toBottomOf="@+id/championtext"
            android:layout_width="match_parent"
            android:layout_height="39dp"
            android:color = "#CDC9C9"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="30dp"
            android:backgroundTint="#EDEDED"
            android:scaleType="fitXY"
            android:background="#CDC9C9"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:textColor="@color/black"
            android:text="我的关注 >"
            android:textSize="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/photo"
            app:layout_constraintStart_toStartOf="@+id/photo"
            app:layout_constraintTop_toTopOf="@+id/photo" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="暂无关注的主播开播哦~"
            android:textColor="#949494"
            android:textSize="13dp"
            android:layout_marginRight="19dp"
            app:layout_constraintBottom_toBottomOf="@+id/photo"
            app:layout_constraintEnd_toEndOf="@+id/photo"
            app:layout_constraintTop_toTopOf="@+id/photo" />
<!--        <androidx.coordinatorlayout.widget.CoordinatorLayout-->
<!--            android:layout_width="match_parent"-->
<!--            android:layout_height="1600dp"-->
<!--            app:layout_constraintBottom_toBottomOf="parent"-->
<!--            app:layout_constraintTop_toBottomOf="@+id/photo"-->
<!--            android:layout_marginTop="10dp">-->
<!--            <com.google.android.material.appbar.AppBarLayout-->
<!--                android:id="@+id/liveLayout"-->
<!--                android:layout_width="match_parent"-->
<!--                android:fitsSystemWindows="true"-->
<!--                android:layout_height="wrap_content"-->
<!--                android:background="@android:color/transparent">-->
<!--                <com.google.android.material.appbar.CollapsingToolbarLayout-->
<!--                    android:id="@+id/collapsingToolbar"-->
<!--                    android:layout_width="match_parent"-->
<!--                    android:layout_height="match_parent"-->
<!--                    app:contentScrim="?attr/colorPrimary"-->
<!--                    app:layout_scrollFlags="scroll|exitUntilCollapsed">-->
<!--                   </com.google.android.mater?-->
                <com.google.android.material.tabs.TabLayout
                    android:id="@+id/liveTab1"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    app:tabTextColor="#949494"
                    android:layout_marginTop="20dp"
                    app:layout_constraintTop_toBottomOf="@+id/photo"
                    app:tabIndicatorHeight="100dp"
                    app:tabIndicatorColor="#DEDEDE"
                    app:tabSelectedTextColor="@color/black" />
<!--            </com.google.android.material.appbar.AppBarLayout>-->
           <FrameLayout
                android:id="@+id/liveframe"
               android:layout_height="0dp"
               android:layout_width="match_parent"
                app:layout_behavior="@string/appbar_scrolling_view_behavior"
                app:layout_constraintTop_toBottomOf="@+id/liveTab1"
                app:layout_constraintBottom_toBottomOf="@id/pppppppppppppppppppp"
                android:layout_margin="10dp" />
<!--        </androidx.coordinatorlayout.widget.CoordinatorLayout>-->
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>
